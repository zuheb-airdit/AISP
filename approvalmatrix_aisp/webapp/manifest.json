{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.approvalmatrix.approvalmatrixaisp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.5",
            "toolsId": "16faca0b-35bc-4426-b808-74793ba2a147"
        },
        "dataSources": {
            "annotation0": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation0.xml",
                "settings": {
                    "localUri": "annotations/annotation0.xml"
                }
            },
            "mainService": {
                "uri": "v2/odata/v4/approval-matrix/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation0"
                    ],
                    "localUri": "localService/mainService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "hierarchysource":{
                "uri": "model/hierarchyModel",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.133.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.approvalmatrix.approvalmatrixaisp.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            },
            "hierarchydata":{
                "dataSource": "hierarchysource",
                "type": "sap.ui.model.json.JSONModel"

            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "beginColumnPages",
                "controlId": "layout",
                "viewType": "XML",
                "async": true,
                "clearControlAggregation": false,
                "viewPath": "com.approvalmatrix.approvalmatrixaisp.view"
            },
            "routes": [
                {
                    "name": "RouteApprovalMatrix",
                    "pattern": ":?query:",
                    "target": [
                        "TargetApprovalMatrix"
                    ]
                },
                {
                    "name": "RouteHierarchyid",
                    "pattern": "RouteHierarchu/{id}/{appType}",
                    "target": [
                        "TargetApprovalMatrix",
                        "TargetApprovalHierarchy"
                    ]
                },
                {
                    "name": "RouteHierarchu",
                    "pattern": "RouteHierarchu",
                    "target": [
                        "TargetApprovalMatrix",
                        "TargetApprovalHierarchy"
                    ]
                }
            ],
            "targets": {
                "TargetApprovalMatrix": {
                    "viewId": "ApprovalMatrix",
                    "viewName": "ApprovalMatrix",
                    "viewLevel": 1,
                    "clearControlAggregation": true
                },
                "TargetApprovalHierarchy": {
                    "viewId": "ApprovalHierarchy",
                    "viewName": "ApprovalHierarchy",
                    "viewLevel": 2,
                    "clearControlAggregation": true,
                    "controlAggregation": "midColumnPages"
                }
            }
        },
        "rootView": {
            "viewName": "com.approvalmatrix.approvalmatrixaisp.view.App",
            "type": "XML",
            "id": "App",
            "async": true
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "aisp_vp_airdit"
    }
}